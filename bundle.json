            "sub_component": [
                "//third_party/backends:third_sane"
            ],
            "inner_kits": [
                {
                    "name": "//third_party/backends:sane",
                    "header": {
                        "header_files": [
                            "sane/sane.h",
                            "sane/saneopts.h"
